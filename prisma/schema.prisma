generator client {
  provider = "prisma-client-js"
  output   = "../api/generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String?   @unique @default(uuid())
  username  String    @unique
  password  String
  createdAt DateTime? @default(now())

  @@index([username])
}
